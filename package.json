{
  "name": "ukelli-ui",
  "version": "4.0.0-alpha0",
  "main": "./index.js",
  "private": true,
  "workspaces": [
    "packages/ui-core",
    "packages/ui-extra",
    "ui-doc",
    "website"
  ],
  "files": [
    "*"
  ],
  "scripts": {
    "publish:ts": "npm publish ./dist",
    "release": "npm run build:ts; npm run publish:ts",
    "build:ts": "rm -rf ./dist; tsc; npm run copy-files-ts; npm run copy-style",
    "copy-files-ts": "node ./scripts/copy-files-ts",
    "copy-style": "cp -R ./style ./dist/",
    "test": "jest",
    "doc": "yarn workspace @ukelli-ui/website dev",
    "clean:doc": "yarn workspace @ukelli-ui/website clean",
    "doc:build": "NODE_ENV=production docz build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ukelli/ukelli-ui.git"
  },
  "keywords": [
    "UI"
  ],
  "author": "Alex",
  "license": "MIT",
  "peerDependencies": {
    "basic-helper": "^3.0.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-transition-group": "^2.5.0",
    "unistore": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "basic-helper": "^3.0.6",
    "codemirror": "^5.42.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "3.0.2",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "fs-extra": "^7.0.0",
    "jest": "^24.5.0",
    "lerna": "^3.18.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-hot-loader": "^4.8.0",
    "react-transition-group": "^4.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.6.4",
    "unistore": "^3.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "flatpickr": "^4.6.3"
  }
}
