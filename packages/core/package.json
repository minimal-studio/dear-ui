{
  "name": "@deer-ui/core",
  "version": "4.0.0-rc6.2",
  "scripts": {
    "build": "npm run get:style:files; npm run copy:files; npm run complie:scss",
    "get:style:files": "sh ./get-style-file.sh",
    "copy:files": "node ./copy-files",
    "complie:scss": "sass --style=compressed --no-source-map ../../packages/core/style/default.scss ../../dist/core/default.css",
    "complie:scss:local": "sass --style=compressed --no-source-map ./style/default.scss ./default.css"
  }
}
