{
  "name": "@deer-ui/core",
  "version": "4.0.0-rc4.2",
  "scripts": {
    "build": "npm run get:style:files; npm run copy:files; npm run complie:scss",
    "get:style:files": "sh ./get-style-file.sh",
    "copy:files": "node ./copy-files",
    "complie:scss": "node-sass ../../packages/core/style/default.scss -o ../../dist/core --output-style compressed",
    "complie:scss:local": "node-sass ./style/default.scss -o ./ --output-style compressed"
  }
}
